function onkopalindromi(sana) {

  let sanavaarinpain = "";
  for (let i = sana.length - 1; i >= 0; i--) {
    sanavaarinpain += sana[i];
  }

  return sana == sanavaarinpain;
}
 
const annasana = prompt("anna sana: ");
const tulos = onkopalindromi(annasana);


if (tulos) {
  console.log(true);
} else {
  console.log(false);
}
